# Root directory containing raw measured metrics
RAW_DATA_ROOT=/absolute/path/to/raw_data_directory
# Root directory for all project-managed data
DATA_ROOT=/absolute/path/to/project_data_root
# Relative filename for anomaly detection dataset
ANOMALY_DATA=example_anomaly_dataset.parquet
# Rated apparent power used for normalization (example value)
ANOMALY_P_RATED=99999
# Example device identifier
SAMPLE_DEVICE=INV_EXAMPLE
# Example site label
SITE_NAME=EXAMPLE_SITE
# Example geographic location
LAT=0.000000
LON=0.000000
# IANA timezone string (example only)
SITE_TZ=Etc/UTC
# ISO-style country code
COUNTRY=XX
# Example device rating
DEVICE_RATING=9999.9
# Month â†’ daylight hour mapping (illustrative only)
# Format: { "month": [start_hour, end_hour] }
DAYLIGHT_MAP_JSON={"1": [0, 24],"2": [0, 24],"3": [0, 24],"4": [0, 24],"5": [0, 24],"6": [0, 24],"7": [0, 24],"8": [0, 24],"9": [0, 24],"10": [0, 24],"11": [0, 24],"12": [0, 24]}
